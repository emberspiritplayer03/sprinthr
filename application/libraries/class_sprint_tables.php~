<?php

class Sprint_Tables {	
	public $table_name;
	
	public function __construct($value) {
		$this->table_name = $value;
	}
	
	public function setTableName($value) {
		$this->table_name = $value;
	}	
	
	public function truncateSelectedTable()
	{
		$sql = 'TRUNCATE TABLE ' . $this->table_name;		
		Model::runSql($sql);
	}
	
